{
    "manifest_version" : 3,
    "name" : "FOCUS TIMER EXTENSION",
    "version" : "1.0",
    "description": "Useful extension for focusing on works and finish them soon.",
    "icons":{
        "16" : "icon.png",
        "48" :"icon.png",
        "128" :"icon.png"
    },
    "permissions":["storage","alarms","notifications"],
    "action" : {
        "default_icon" : "icon.png",
        "default_title" :"FOCUS TIMER",
        "default_popup" : "popup/popup.html"
    },
    "background" : {
        "service_worker":"background.js"
    },
    "options_page" : "options/options.html"
}